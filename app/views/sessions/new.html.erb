<div class="row">
	<div class="col-xs-3">
		<img class="bt-sign-up-img" src="https://s3-us-west-1.amazonaws.com/beat-tree-images/bt-logo.gif">
	</div>
	<div class="col-xs-9 sign-up-explore">
		<h1>Sign In.</h1>
	</div>
</div>
<hr>
<div class="row">
	<div class="col-xs-4 col-xs-offset-4">
	<form class="sign-in-data" action="<%= session_url %>" method="post">
		<div class="form-group">
			<label for="username-field">Username</label>
			<input type="text" id="username-field" class="form-control" name="user[username]">
		</div>
		<div class="form-group">
			<label for="password-field">Password</label>
			<input type="password" class="form-control" id="password-field" name="user[password]">
		</div>
		<div class="form-group">
			<%= button_to "Sign In", session_url, method: :post %>
		</div>
	</form>
	</div>
</div>